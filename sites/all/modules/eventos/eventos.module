  $redirect = $aredirect[count($aredirect)-2].'/'.$profissional;
  
  $form['nome'] = array(
    '#type' => 'textfield', 
    '#autocomplete_path' => 'entityreference/autocomplete/single/field_paciente/node/agendamento/NULL',
    '#title' => 'Nome do paciente',
