  } else {
    $nid = $_GET['nid'];
  }

  $agendamento = node_load($nid);
  foreach($agendamento->field_procedimentos['und'] as $k=>$v){

    $proc = node_load($v['target_id']);

    $objProcedimento[$k]->id            = $v['target_id'];
    $objProcedimento[$k]->Procedimento  = $proc->title;
